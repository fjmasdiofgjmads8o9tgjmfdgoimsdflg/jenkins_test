#!groovyfsdf
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '3', artifactNumToKeepStr: '3'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@172.18.0.3 \'echo "test"\''
            }
        }
    stage("Second step") {
            steps {
                sh 'ssh root@172.18.0.3 \'echo "test3"\''
            }
        }
    }
}
