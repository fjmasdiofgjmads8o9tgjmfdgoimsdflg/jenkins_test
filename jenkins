#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
          buildDiscarder([$class: 'BuildRotator', artifactsDaysToKeep: 999, artifactsNumToKeep: 3, daysToKeep: 999, numToKeep: 3])
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@172.18.0.3 \'echo "test"\''
            }
        }
    stage("Second step") {
            steps {
                sh 'ssh root@172.18.0.3 \'echo "test3"\''
            }
        }
    }
}
